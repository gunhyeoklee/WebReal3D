{
  "name": "@web-real/core",
  "type": "module",
  "main": "dist/web-real-core.cjs",
  "module": "dist/web-real-core.mjs",
  "types": "dist/types/index.d.ts",
  "files": ["dist"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/web-real-core.mjs",
      "require": "./dist/web-real-core.cjs"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && vite build --config vite.config.ts",
    "test": "vitest run --config vitest.config.ts"
  },
  "dependencies": {
    "@web-real/math": "workspace:*"
  }
}
