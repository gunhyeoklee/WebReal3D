{
  "name": "web-real",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.0",
  "workspaces": [
    "packages/math",
    "packages/core",
    "examples/*"
  ],
  "scripts": {
    "build:core": "bun --filter './packages/core' build",
    "build:math": "bun --filter './packages/math' build",
    "build:all": "bun run build:math && bun run build:core"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@webgpu/types": "^0.1.66",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  }
}
